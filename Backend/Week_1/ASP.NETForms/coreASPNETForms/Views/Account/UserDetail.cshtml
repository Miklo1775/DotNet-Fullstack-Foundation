@model coreASPNETForms.Models.LoginView

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UserDetail</title>
</head>
<body>
    <h2>User Details</h2>
    <p>Username: @Model.Username</p>
    <p>Password: @Model.Password</p>

    <a asp-controller="Account" asp-action="UserList">See All Users</a>
</body>
</html>
